{
    "name": "vladkens/autoprefixer",
    "type": "library",
    "description": "Run Autoprefixer using node.js in you php.",
    "keywords": ["autoprefixer", "css", "prefixes", "node.js"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vlad Pronsky",
            "email": "vladkens@yandex.ru"
        }
    ],
    "support": {
        "email": "vladkens@yandex.ru",
        "issues": "https://github.com/vladkens/autoprefixer-php/issues",
        "source": "https://github.com/vladkens/autoprefixer-php"
    },
    "require": {
        "php": ">=5.3.0",
        "mouf/nodejs-installer": "~1.0"
    },
    "extra": {
        "mouf": {
            "nodejs": {
                "includeBinInPath": true
            }
        }
    },
    "autoload": {
        "psr-0": {
            "": "lib"
        }
    }
}
